{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "require-jsdoc": "off",
    "linebreak-style": ["error", "unix"],
    "no-invalid-this": "off"
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}